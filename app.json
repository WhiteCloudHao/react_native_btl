{
  "expo": {
    "name": "facebook clone",
    "slug": "multiple_platform_project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "userInterfaceStyle": "light",
    "scheme": "mppbtl",
    "splash": {
      "image": "./assets/iphone-splash-preview-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.multiple_platform_project"
    },
    "web": {
      "favicon": "./assets/images/app_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5aeceb08-43e4-40bf-9dd9-c03a4d10fd37"
      }
    }
  }
}
